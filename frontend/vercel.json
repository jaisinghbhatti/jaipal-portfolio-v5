{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(bot|crawler|spider|crawling|facebookexternalhit|LinkedInBot|Twitterbot|Slackbot|WhatsApp).*"
        }
      ],
      "destination": "https://service.prerender.io/https://jaisingh.in/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Prerender-Token",
          "value": "YOUR_PRERENDER_TOKEN_HERE"
        }
      ]
    }
  ]
}
